<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="view.ui.ProgramController">
    <children>
        <HBox alignment="CENTER">
            <children>
                <HBox alignment="CENTER" prefHeight="200.0" prefWidth="280.0">
                    <children>
                        <VBox prefHeight="134.0" prefWidth="143.0">
                            <children>
                                <HBox alignment="CENTER" prefHeight="31.0" prefWidth="150.0">
                                    <children>
                                        <Label prefHeight="64.0" prefWidth="81.0" text="Program States">
                                            <HBox.margin>
                                                <Insets bottom="5.0" left="5.0" right="5.0" top="10.0"/>
                                            </HBox.margin>
                                        </Label>
                                        <Label fx:id="programStateNumber" text="5">
                                            <HBox.margin>
                                                <Insets bottom="5.0" left="5.0" right="5.0" top="10.0"/>
                                            </HBox.margin>
                                        </Label>
                                    </children>
                                </HBox>
                                <ListView fx:id="programStateList" prefHeight="187.0" prefWidth="280.0">
                                    <VBox.margin>
                                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                                    </VBox.margin>
                                </ListView>
                            </children>
                        </VBox>
                        <VBox alignment="CENTER" prefHeight="121.0" prefWidth="128.0">
                            <children>
                                <Button mnemonicParsing="false" onAction="#oneStep" prefHeight="25.0" prefWidth="88.0"
                                        text="One Step">
                                    <HBox.margin>
                                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                                    </HBox.margin>
                                    <VBox.margin>
                                        <Insets bottom="5.0" left="5.0" right="5.0" top="40.0"/>
                                    </VBox.margin>
                                </Button>
                                <Button mnemonicParsing="false" onAction="#allSteps" prefHeight="25.0" prefWidth="88.0"
                                        text="All Steps">
                                    <HBox.margin>
                                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                                    </HBox.margin>
                                    <VBox.margin>
                                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                                    </VBox.margin>
                                </Button>
                                <Button mnemonicParsing="false" onAction="#openLog" prefHeight="25.0" prefWidth="88.0"
                                        text="View Log">
                                    <VBox.margin>
                                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                                    </VBox.margin>
                                </Button>
                                <Button fx:id="backButton" mnemonicParsing="false" onAction="#back" prefHeight="25.0"
                                        prefWidth="88.0" text="Back">
                                    <VBox.margin>
                                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                                    </VBox.margin>
                                </Button>
                            </children>
                        </VBox>
                    </children>
                </HBox>
                <VBox alignment="CENTER" prefHeight="200.0" prefWidth="379.0">
                    <children>
                        <Label text="Execution Stack">
                            <VBox.margin>
                                <Insets bottom="5.0" left="5.0" right="5.0" top="10.0"/>
                            </VBox.margin>
                        </Label>
                        <ListView fx:id="executionStack" prefHeight="143.0" prefWidth="606.0">
                            <VBox.margin>
                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                            </VBox.margin>
                        </ListView>
                    </children>
                </VBox>
            </children>
        </HBox>
        <HBox alignment="CENTER">
            <children>
                <VBox alignment="CENTER" prefHeight="200.0" prefWidth="182.0">
                    <children>
                        <Label text="Symbol Table">
                            <VBox.margin>
                                <Insets left="10.0" right="10.0" top="10.0"/>
                            </VBox.margin>
                        </Label>
                        <TableView fx:id="symbolTable" prefHeight="153.0" prefWidth="185.0">
                            <VBox.margin>
                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                            </VBox.margin>
                        </TableView>
                    </children>
                </VBox>
                <VBox alignment="CENTER" prefHeight="200.0" prefWidth="182.0">
